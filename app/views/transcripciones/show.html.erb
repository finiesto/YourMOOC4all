<%- model_class = Transcripcion -%>
<div class="col-md-12">
  <div class="well col-md-12 ">
    <dl class="dl-horizontal">
      <dt>Curso:</dt>
      <dd><%= @transcripcion.curso %></dd>
      <dt>Idioma:</dt>
      <dd><%= @transcripcion.idioma %></dd>

    </dl>

    <div class="panel-footer">
      <%= link_to transcripciones_path, :class => 'btn btn-default' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        <%= t('.back', :default => t("helpers.links.back", :default => 'Volver')) %>
      <% end %>
      <%= link_to edit_transcripcion_path(@transcripcion), :class => 'btn btn-primary' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        <%= t('.edit', :default => t("helpers.links.edit", :default => 'Editar')) %>
      <% end %>

      <%= link_to transcripcion_path(@transcripcion), :method => 'delete', :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => "Â¿Eliminar el transcripcion '#{@transcripcion.id}'?")) }, :class => 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-trash"></span>
        <%= t('.destroy', :default => t("helpers.links.destroy")) %>
      <% end %>
    </div>
  </div>
</div>

  
                                
    
    
    
    
                                
    
    
    
  