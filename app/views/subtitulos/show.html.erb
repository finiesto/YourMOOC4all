<%- model_class = Subtitulo -%>
<div class="col-md-12">
  <div class="well col-md-12 ">
    <dl class="dl-horizontal">
      <dt><%= t('mio.evaluacion_curso') %>:</dt>
      <dd><%= @subtitulo.curso %></dd>
      <dt><%= t('mio.evaluacion_idioma') %>:</dt>
      <dd><%= @subtitulo.idioma %></dd>
    </dl>

    <div class="panel-footer">
      <%= link_to subtitulos_path, :class => 'btn btn-default' do %>
        <span class="glyphicon glyphicon-list-alt"></span>
        <%= t('.back', :default => t("helpers.links.back", :default => 'Volver')) %>
      <% end %>
      <%= link_to edit_subtitulo_path(@subtitulo), :class => 'btn btn-primary' do %>
        <span class="glyphicon glyphicon-pencil"></span>
        <%= t('.edit', :default => t("helpers.links.edit", :default => 'Editar')) %>
      <% end %>

      <%= link_to subtitulo_path(@subtitulo), :method => 'delete', :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => "Â¿Eliminar el subtitulo '#{@subtitulo.id}'?")) }, :class => 'btn btn-danger' do %>
        <span class="glyphicon glyphicon-trash"></span>
        <%= t('.destroy', :default => t("helpers.links.destroy")) %>
      <% end %>
    </div>
  </div>
</div>

  
                                
    
    
    
    
                                
    
    
    
  